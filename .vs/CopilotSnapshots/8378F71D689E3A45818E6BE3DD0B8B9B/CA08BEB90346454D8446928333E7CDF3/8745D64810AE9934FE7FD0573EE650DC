using ExcelReader.RyanW84.Models;
using Microsoft.EntityFrameworkCore;

namespace ExcelReader.RyanW84.Data;

public interface IExcelReaderDbContext
{
    DbSet<ExcelBeginner> ExcelBeginner { get; set; }
    new Task<int> SaveChangesAsync(); // Use 'new' to hide base method
    void EnsureDeleted();
    void EnsureCreated();
}
